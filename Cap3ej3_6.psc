	n_pi<-3.14;
	Escribir "Lados a y b del rectangulo";
		leer a,b;
	Escribir "Radio del circulo";
	    leer r;
			areaRectangulo<- a * b;
			areaCirculo<- n_pi * (r*r);
	Escribir "El area del Rectangulo es: ",areaRectangulo;
	Escribir "El area del Circulo es: ",areaCirculo;
	        areaAchurada <- areaRectangulo - areaCirculo;
	Escribir "Y el area Achurada es de: ",areaAchurada;
FinProceso